									"});",
									"",
									"pm.test(\"Body matches uploader username\", function () {",
									"    pm.expect(pm.response.text()).to.include(pm.variables.get(\"USERNAME2\"));",
									"});",
									"",
									"// Load cheerio library and parse response",
									"const $ = cheerio.load(pm.response.text());",
									"var rows = $('th');",
									"",
									"// Get username2 score",
									"pm.globals.set(\"username2_score\", -1);",
									"for (i = 0; i < rows.length; i++) {",
									"    var row = rows[i];",
									"    if (row.children[0].data == pm.variables.get(\"USERNAME2\")) {",
									"        // Found",
									"        var scoreRow = rows[i+1].children[0].data;",
									"        pm.globals.set(\"username2_score\", scoreRow);",
