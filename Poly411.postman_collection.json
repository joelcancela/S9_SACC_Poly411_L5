								"exec": [
									"pm.test(\"response must be valid\", function () {",
									"    pm.response.to.be.ok;",
									"});",
									"",
									"pm.test(\"Body matches download string\", function () {",
									"    pm.expect(pm.response.text()).to.include(\"<a href='https://storage.googleapis.com/polar-winter-218511/\");",
									"});",
									"",
									"// Load cheerio library and parse response",
									"const $ = cheerio.load(pm.response.text())",
									"",
									"// Resolve download link",
									"pm.globals.set(\"download_link\", $('a').text());",
									"console.info(pm.globals.get(\"download_link\"));",
									"",
									"// Try to download the file",
									"pm.sendRequest(pm.globals.get(\"download_link\"), function (err, response) {",
									"    pm.test(\"downloading is ok\", function () {",
									"        pm.response.to.be.ok;",
									"    });",
									"});",
