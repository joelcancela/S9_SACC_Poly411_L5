									"const $ = cheerio.load(pm.response.text());",
									"var rows = $('th');",
									"",
									"// Get username2 score",
									"pm.globals.set(\"username2_score_updated\", -1);",
									"for (i = 0; i < rows.length; i++) {",
									"    var row = rows[i];",
									"    if (row.children[0].data == pm.variables.get(\"USERNAME2\")) {",
									"        // Found",
									"        var scoreRow = rows[i+1].children[0].data;",
									"        pm.globals.set(\"username2_score_updated\", scoreRow);",
